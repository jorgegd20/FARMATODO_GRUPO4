        <div class="container">
            &copy; @DateTime.Now.Year=> @DateTime.Now.Date | @DateTime.Now.Day/@DateTime.Now.Month/@DateTime.Now.Year - FarmatodoWeb - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
